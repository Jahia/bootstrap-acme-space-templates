                                     startDateProperty="startDate"/>
                                        <facets j:bindedComponent="/modules/bootstrap-acme-space-templates/templates/base/events/pagecontent/content/content/events"
                                   j:type="jnt:event"
                                   jcr:primaryType="jnt:facets">
                                            <fieldFacet facet="type"
                                          field="jnt:event;eventsType"
                                          jcr:primaryType="jnt:fieldFacet"
                                          limit="100"
                                          mincount="1"
                                          missing="false"
                                          offset="0">
                                                <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Type"/>
                                            </fieldFacet>
                                            <field-hierarchical-facet facet="Categories"
                                                        field="jmix:categorized;j:defaultCategory"
                                                        jcr:primaryType="jnt:fieldHierarchicalFacet"
                                                        limit="100"
                                                        mincount="1"
                                                        missing="false"
                                                        offset="0"
                                                        prefix="/sites/systemsite/categories">
                                                <j:translation_en jcr:language="en"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Categories"/>
                                                <j:translation_fr jcr:language="fr"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Catégories"/>
                                            </field-hierarchical-facet>
                                            <queryFacet facet="nextEvents"
                                          jcr:primaryType="jnt:queryFacet"
                                          mincount="1"
                                          query="0\:FACET\:startDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/DAY TO NOW/MONTH+1MONTH-1SECOND]">
                                                <j:translation_en jcr:language="en"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Next events per month"
                                                   valueLabel="##DynamicDateLabel(NOW/MONTH,date,MMMM)##"/>
                                                <j:translation_fr jcr:language="fr"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Prochains évènements par mois"
                                                   valueLabel="##DynamicDateLabel(NOW/MONTH,date,MMMM)##"/>
                                            </queryFacet>
                                            <queryFacet-1 facet="nextEvents"
                                            jcr:primaryType="jnt:queryFacet"
                                            mincount="1"
                                            query="0\:FACET\:startDate:[NOW/MONTH+1MONTH TO NOW/MONTH+2MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/MONTH+1MONTH TO NOW/MONTH+2MONTH-1SECOND]">
                                                <j:translation_en jcr:language="en"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Next events per month"
                                                   valueLabel="##DynamicDateLabel(NOW/MONTH+1MONTH,date,MMMM)##"/>
                                                <j:translation_fr jcr:language="fr"
                                                   jcr:primaryType="jnt:translation"
                                                   label="Prochains évènements par mois"
                                                   valueLabel="##DynamicDateLabel(NOW/MONTH+1MONTH,date,MMMM)##"/>
                                            </queryFacet-1>
                                            <queryFacet-2 facet="nextEvents"
                                            jcr:primaryType="jnt:queryFacet"
                                            mincount="1"
                                            query="0\:FACET\:startDate:[NOW/MONTH+2MONTH TO NOW/MONTH+3MONTH-1SECOND] OR 0\:FACET\:endDate:[NOW/MONTH+2MONTH TO NOW/MONTH+3MONTH-1SECOND]">
